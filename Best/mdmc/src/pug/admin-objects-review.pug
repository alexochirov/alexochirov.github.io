extends user-personal

block personal
  .row.column
    - var n=0
    while n < 5
      - n++
      .callout
        .row
          .medium-8.large-10.column
            h3: a(href='object.html' title='На страницу объекта') Название объекта #{n}
            p
              | Категория объекта 1, Категория объекта 2, Категория объекта 3
              br
              | #[img(src='./i/Flag_of_Russia.svg' alt='' style='height: 18px; 1px solid #ddd;')] Россия
              br
              | #[span.text-dark-gray Адрес: ] г. Ярославль, пр-т Ленина 25
              br
              | #[span.text-dark-gray Тел.: ] +7 (499) 918-43-57
              br
              | #[span.text-dark-gray Сайт: ] www.somewebsite.com
          .show-for-medium.medium-4.large-2.column
            img(src='./i/map.png' alt='Фото объекта')

        div
          a.button(href='') Принять
          | &#160; &#160;
          a.button.secondary(href='') Отклонить

    p &nbsp;
    include components/common/_pagination
