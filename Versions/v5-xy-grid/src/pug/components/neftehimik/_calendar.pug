.reveal(id='recentGameModal' data-reveal data-deep-link='true')
    include ../smi/_recent-game-for-data-reveal
    button.close-button(data-close aria-label='Закрыть' type='button')
        span(aria-hidden='true') &times;

.reveal(id='upcomingGameModal' data-reveal data-deep-link='true')
    include ../smi/_upcoming-game-for-data-reveal
    button.close-button(data-close aria-label='Закрыть' type='button')
        span(aria-hidden='true') &times;
