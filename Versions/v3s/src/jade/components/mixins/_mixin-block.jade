mixin stub(title)
    .stub&attributes(attributes)
        .stub-title
            h2.text-white= title
//-mixin _mixin-block(title)
    div(style!=attributes.style+' position:relative;')
        img(src='./images/image.svg', alt='', style='background:#ccc;width:100%;height:100%;position:absolute;top:0;left:0;')
        .vertical-middle.text-center
            h2.text-white= title